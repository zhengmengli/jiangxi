.pager{
  width: auto;
}

.blank{
  height: 20px;
  width:20%;
  margin: 0px;
  padding: 0px;
  float: left;
}
.ng-pagination {
  font-family: 'Microsoft YaHei';
  font-size: 12px;
  color: #0582d2;
}

.ng-pagination>ul {
  display: block;
  margin: 0;
  padding: 0;
  list-style: none;
}

.ng-pagination>ul>li {
  padding: 2px 6px;
  margin-right: 5px;
  border: 1px solid #cccccc;
  cursor: pointer;
  background: none;
  float: left;
  color: #666666;
}

.ng-pagination>ul>li:hover,
.ng-pagination>ul>li.active {
  background-color: #39d5b0;
  color: #ffffff;
}
.ng-pagination>ul>li.firstLast{
  background-color: #0582d2;
  color: #ffffff;
}


.ng-pagination>ul>li.showGoto {
  background:none;
  border-radius:0px;
}

.ng-pagination>ul>li.showGoto span.pageCount{
  margin-left:3px;
  line-height:0px;
  color:#666666;
  font-size:13px;
}

.ng-pagination>ul>li.showGoto>lable input {
  margin-left: 2px;
  width:20px;
  height:16px;
  line-height:0px;
  border-radius: 16px;
  border: 1px solid #666666;
  vertical-align: top;
  background:none;
  color:#666666;
  font-size:12px;
  text-align:center;
  padding:0 3px;
}
