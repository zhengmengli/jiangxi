.pjbg,.pjbg table{
	width: 100%;
}
.selUpDailogCheck .active{
	background: #1E70E9;
}
.pjbg table tr td{
	/*width:10%;*/
	height: 60px;
	border:none;
	font-family: PingFangSC-Semibold;
	font-size: 14px;
	color: #434549;
	letter-spacing: 0;
	text-align: center;
}
.pjbg table tr{
	border: 1px solid #eee;
	height: 60px;
}

.pjbg table tr:nth-child(odd){
	background: #F0F1F1;
}
.pjbg table tr:nth-child(even){
	background: #fff;
}
