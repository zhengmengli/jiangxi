    background: white;
    cursor: pointer;
}
.evaluate-box .evaluate-list ul li.active{
    background: #1E70E9;
    color: white;
}
.no-border{
    border: none;
}

.time-box{
    position: absolute;
    width: 100%;
    bottom: 0;
    left: 0;
    height: 8px;
}
.time-box .time-process{
    position: relative;
    height: 8px;
    background: #23A25A;
}
.time-box .time-process .timer{
    position: absolute;
    right: -24px;
    top: -9px;
    width: 28px;
    height: 28px;
    background: url(../img/ktgc/timer.png) center no-repeat;
}
.time-box .time-process .time-num{
    position: absolute;
    right: -44px;
    top: -42px;
    width: 68px;
    height: 40px;
    line-height: 32px;
    font-size: 15px;
    color: #FFB430;
    text-align: center;
    background: url(../img/ktgc/dialog.png) center no-repeat;
}
.seat-box{
    position: absolute;
    width: calc(100% - 100px);
    width: -moz-calc(100% - 100px);
    width: -webkit-calc(100% - 100px);
    top: 50px;
    left: 50px;
    height: 600px;
    background: #FFFFFF;
    box-shadow: 0 0 4px 2px rgba(0,0,0,0.06);
}
.seat-box .seat-title{
    width: 300px;
    height: 100px;
    margin: 0 auto;
    margin-bottom: 50px;
    background: url(../img/ktgc/u2.png) center no-repeat;
}

.seat-box ul{
    position: relative;
    padding: 0 80px;
}
.seat-box .seat{
    float: left;
    width: 60px;
    height: 60px;
    text-align: center;
    font-size: 13px;
    margin: 0 40px 30px 0;
    cursor: pointer;
}
.seat-box .seat img{
    width: 50px;
}
.seat-box .record{
    position: absolute;
    top: 100px;
    left: 190px;;
    width: 280px;
    height: 350px;
    z-index: 9;
    background: #FFFFFF;
    box-shadow: 0 0 4px 2px rgba(0,0,0,0.22);
}
.seat-box .record .name{
    line-height: 40px;
    padding-left: 30px;
    color: white;
    background: #1E70E9;
}
.seat-box .close-ico{
    position: absolute;
    top: -13px;
    right: -13px;
    width: 30px;
    height: 30px;
    background: url(../img/login/close.png) center no-repeat;
    cursor: pointer;
}

.seat-box .hand-box,.seat-box .count-box{
    width: 210px;
    height: 80px;
    text-align: center;
    margin: 0 auto;
    margin-top: 30px;
}
.seat-box .record .hand{
    width: 70px;
    height: 70px;
    padding: 34px 0 0 18px;
    text-align: center;
    font-size: 15px;
    color: white;
    background: url(../img/ktgc/gc-hand.png)  center no-repeat;
}
.seat-box .record .count-box .hand{
    padding-top: 22px;
    background: url(../img/ktgc/gc-question.png) 14px center no-repeat;
    background-size: 81%; 
}
.seat-box .record .count-box .result{
    margin-top: 5px;
    text-align: center;
}
.seat-box .record .count-box .result .radio1{
    position: relative;
    top: 5px;
    display: inline-block;
    width: 18px;
    height: 18px;
    background: url(../img/ktgc/radio1.png) center no-repeat;
}
.seat-box .record .count-box .result .radio1.check{
    background: url(../img/ktgc/radio.png) center no-repeat;
}
.seat-box .record .count-box .result .radio1 input{
    position: absolute;
    top: 0;
    left: 0;
    width: 18px;
    height: 18px;
    margin: 0;
    opacity: 0;
    filter: alpha(opacity=0);
}
.seat-box .record .count-box .result label{
    margin: 0;
    cursor: pointer;
}
.seat-box .record .count-input{
    margin: 24px 0 0 20px;
}
.seat-box .record .count-box .count-input{
    margin-top: 0;
}
.seat-box .record .count-input>div{
    width: 30px;
    height: 30px;
    line-height: 30px;
    font-size: 20px;
