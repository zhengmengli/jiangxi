html,
body {
    width: 100%;
    height: 100%;
}

body {
    min-width: 1000px;
    background: #F2F2F2;
}

* {
    outline: 0 !important;
}

ul,
li {
    margin: 0;
    padding: 0;
    list-style: none;
}

a {
    color: #333;
    cursor: pointer;
    text-decoration: none;
}

a:hover,
a:visited,
a:focus {
    text-decoration: none;
}

.fl {
    float: left;
}

.fr {
    float: right;
}
.side-nav ul li .child-menu{
    line-height: 46px;
    padding-left: 50px; 
    background: #222222;
    color: #C0C4C8;
    font-size: 16px;
    text-align: left;
}
.side-nav ul li .child-menu.active,.side-nav ul li .child-menu:hover{
    color: white;
}
.clearfix:after {
    clear: both;
    content: ".";
    display: block;
    font-size: 0;
    height: 0;
    line-height: 0;
    visibility: hidden;
}
.overflow-dots {
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.homeTitle{
	font-size: 22px;
	color: #434549;
	width: 100%;
	text-align: center;
	position: absolute;
	top: 18%;
}

.head-body{
    height: 70px;
}
.head-nav {
    position: fixed;
    width: 100%;
    height: 70px;
    background-color: #447dfe;
    z-index: 10;
    line-height: 70px;
    color: white;
}
.head-nav .left{
    position: relative;
    float: left;
}
.head-nav .circle{
    position: absolute;
    top: 15px;
    left: 40px;
    width: 47px;
    height: 43px;
    /*background: #D8D8D8;*/
    background: url(../img/ktgc/icon-pc1x.png)no-repeat center;
    /*border: 1px solid #979797;*/
    border-radius: 50%;
    
}
.head-nav .logo-name{
    display: inline-block;
    font-size: 26px;
    margin-left: 120px;
}
.head-nav .user-info{
    margin-right: 90px;
}
.head-nav .user-info .user-img{
    float: left;
    width: 48px;
    height: 48px;
    margin: 11px;
    background: url(../img/user.png)no-repeat center;
}
.head-nav .logout{
    position: absolute;
    top: 0;
    right: 0;
    width: 70px;
    height: 70px;
    cursor: pointer;
    background-color: #3f77f3;
}
.head-nav .tuichu{
    position: absolute;
    left: 50%;
    width: 70px;
    margin-left:-14px;
}
.head-nav .logout .tuichu i{
    font-size:26px;
    color:#333;
}
.head-nav .logout:hover{
    background:#4393fd;
}
.head-nav .logout:hover .tuichu i{
    color:#fff;
}
.head-nav .user-menu{
    position: fixed;
    top: 82px;
    right: 52px;
    width: 160px;
    background: white;
    box-shadow: 0 2px 4px 0 rgba(0,0,0,0.10);
}
.head-nav .user-menu .arrow-t{
    position: absolute;
    top: -23px;
    right: 20px;
    border: 10px solid transparent;
    border-bottom: 20px solid white;
    width: 0;
    height: 0px;
}
.head-nav .user-menu ul{
    /*padding: 15px 0;*/
}
.head-nav .user-menu ul li{
    font-size: 14px;
    color: #434549;
    line-height: 36px;
    text-align: center;
    cursor: pointer;
}
.head-nav .user-menu ul li:hover{
    color: white;
    background: #1E70E9;
}
.head-nav .user-menu .exit{
    line-height: 32px;
    font-size: 13px;
    color: #FFFFFF;
    text-align: center;
    background: #FFB430;
    cursor: pointer;
}

/* 主导航 */

.side-nav {
    position: fixed;
    left: 0;
    top: 70px;
    background: #2C2C2C;
    height: 100%;
    z-index: 5;
}
.side-nav ul{
    width: 120px;
    margin-top: 40px;
}
.side-nav ul li{
    position: relative;
    width: 100%;
    /*height: 90px;*/
    text-align: center;
    cursor: pointer;
}
.side-nav ul li a{
    display: block;
    width: 120px;
    height: 90px;
    color: #C0C4C8;
    
}
.side-nav ul li a.active,.side-nav ul li a:hover{
    color: white;
    background: #222222;
}
.side-nav ul li img{
    margin-top: 20px;
    width: 30px;
    height: 30px;
    padding: 4px;
}
.side-nav ul li .nav-name{
    line-height: 20px;
    font-size: 14px;
    text-align: center;
}

.side-nav ul li .bar{
    position: absolute;
    top: 0;
    left: 0;
    width: 4px;
    height: 100%;
    background: #FFB430;
}

