    color: #838C95;
    padding-left: 30px;
    border-bottom: 1px solid #F0F1F1;
    cursor: pointer;
}
.quota-box ul li.active{
    color: white;
    background: #1E70E9;
}
.quota-box ul li .dot{
    position: absolute;
    top: 20px;
    left: 10px;
    width: 10px;
    height: 10px;
    border-radius: 50%;
}
.quota-box ul li .dot.white-dot{
    background: white;
}
.quota-box ul li .dot.blue-dot{
    background: #1E70E9;
}

.evaluate-box{
    width: 40%;
}
.evaluate-box .evaluate-list{
    padding: 20px;
    background: #FFFFFF;
    border: 1px solid #E7E7E7;
    border-radius: 2px;
}
.evaluate-box .evaluate-list ul li{
    float: left;
    line-height: 32px;
    padding: 0 18px;
    margin: 0 20px 20px 0;
    font-size: 13px;
    color: #C0C4C8;
    border: 1px solid #C0C4C8;
