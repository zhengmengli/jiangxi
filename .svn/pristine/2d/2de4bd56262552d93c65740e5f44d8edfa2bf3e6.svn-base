<!DOCTYPE html>
<html class="um landscape min-width-240px min-width-320px min-width-480px min-width-768px min-width-1024px">

<head>
    <title>
    </title>
    <meta charset="utf-8">
    <meta name="viewport" content="target-densitydpi=device-dpi, width=device-width, initial-scale=1, user-scalable=no, minimum-scale=1.0, maximum-scale=1.0">
    <link rel="stylesheet" href="css/fonts/font-awesome.min.css">
    <link rel="stylesheet" href="css/ui-box.css">
    <link rel="stylesheet" href="css/ui-base.css">
    <link rel="stylesheet" href="css/ui-color.css">
    <link rel="stylesheet" href="css/appcan.icon.css">
    <link rel="stylesheet" href="css/appcan.control.css">
    <link rel="stylesheet" href="./css/index.css">
    <link rel="stylesheet" href="./css/my.css">

    </style>
</head>

<body class="" ontouchstart>
    <div class="head-bar">
        <span class="left-icon">取消</span>
        <span style="float:left;margin-left:0.5rem;"></span>
        <span>综合评价</span>
        <span class="right-icon">完成</span>
    </div>
    <div class="panel clearfix" style="background:#F7F9FC;overflow:auto;">
        <section class="edit-container" id="cur-list">
            <div class="edit-title">
                <span class="left-line"></span>
                <span class="left-name">我的一级指标</span>
            </div>
            <ul>
                <li class="clearfix">
                    <span class="name">情景分析</span>
                    <span class="s-button delete">移除</span>
                    <span class="s-button copy">复制</span>
                </li>
                <li class="clearfix">
                    <span class="name">现场模拟</span>
                    <span class="s-button delete">移除</span>
                </li>
                <li class="clearfix">
                    <span class="name">分析还原</span>
                    <span class="s-button delete">移除</span>
                    <span class="s-button copy">复制</span>
                </li>
            </ul>
        </section>
        <section class="edit-container" id="more-list">
            <div class="edit-title">
                <span class="left-line"></span>
                <span class="left-name">未添加的一级指标</span>
            </div>
            <ul>
                <li class="clearfix">
                    <span class="name">一级指标</span>
                    <span class="s-button add">添加</span>
                </li>
                <li class="clearfix">
                    <span class="name">一级指标一</span>
                    <span class="s-button add">添加</span>
                </li>
                <li class="clearfix">
                    <span class="name">一级指标二</span>
                    <span class="s-button add">添加</span>
                </li>
            </ul>
        </section>
    </div>

    <script src="js/appcan.js">
    </script>
    <script src="js/appcan.control.js">
    </script>
    <script src="js/appcan.scrollbox.js">
    </script>
    <script src="js/template.import.js">
    </script>
    <script src="./assets/mvvm/index.js">
    </script>
    <script src="./js/index.js">
    </script>
    <script type="text/javascript">
        $('.left-icon').bind('click', function () {
            appcan.window.close(-1);
        });
        $('.right-icon').bind('click', function () {
            appcan.window.open("list", "list.html");
        });
        $('#cur-list').bind('click', function (event) {
            if (event.target.className.indexOf('copy') > -1) {
                var name = $(event.toElement).siblings('.name')[0].innerText;
                var name1 = $(event.toElement).siblings('.name')[0].innerText.replace(/[0-9]/ig, "");
                var num = name.replace(/[^0-9]/ig, "");
                if (num == '') {
                    num = 1;
                } else {
                    num = parseInt(num) + 1;
                }
                name1 += num;
                var str = $("<li class='clearfix'>" + "<span class='name'>" + name1 + "</span>" +
                    "<span class='s-button delete'>移除</span>" +
                    "<span class='s-button copy'>复制</span><li>")[0];
                $('#cur-list ul').append(str);
                $(event.toElement).remove();
            }
            if (event.target.className.indexOf('delete') > -1) {
                $(event.toElement).parent('li')[0].remove();
            }
        });
        $('#more-list').bind('click', function (event) {
            if (event.target.className.indexOf('add') > -1) {
                $(event.toElement).parent('li')[0].remove();
                var str = $("<li class='clearfix'>" + "<span class='name'>" + $(event.toElement).siblings('.name')[0].innerText + "</span>" +
                    "<span class='s-button delete'>移除</span>" +
                    "<span class='s-button copy'>复制</span><li>")[0];
                $('#cur-list ul').append(str);
            }
        })
    </script>

</body>

</html>