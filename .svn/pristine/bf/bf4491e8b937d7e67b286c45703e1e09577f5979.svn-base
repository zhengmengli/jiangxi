    <script type="text/javascript">
        $('.back').bind('click', function () {
            appcan.window.close(-1); 
        });
        $('.edit').bind('click', function () {
            $('#toast').toggle();
        });
        $('#toast ').bind('click', function () {
            appcan.window.open("zhibiao","zhibiao.html");
        });
        $('.menu').bind('click', function () {
            $('.menu').removeClass('active');
            $(this).addClass('active');
        });
        $('.quota-list li').bind('click', function () {
            $('.quota-list li').removeClass('active');
            $(this).addClass('active');
        });
        $('.zhibiao-content').each(function (index) {
            var that = $(this);
            $(this).find('li').bind('click', function () {
                if ($(this).find('span.radio').length !== 0) {
                    that.find('li').removeClass('active');
                    $(this).addClass('active');
                } else {
                    if ($(this).prop("className").indexOf('active') > -1) {
                        $(this).removeClass('active');
                    } else {
                        $(this).addClass('active');
                    }
                }

            });
        });

        $('.more-icon').bind('click', function () {
            $('.quota-poup').toggle();
        });
        $('.quota-poup').bind('click', function (event) {
            $('.quota-poup').hide();
        });
        $('.quota-poup li').bind('click', function (event) {
            $('.quota-poup li').removeClass('active');
            $(this).addClass('active');
            $('.quota-poup').hide();
            event.stopPropagation();
        });

        // 悬浮按钮
        function init() {
            var ele = document.getElementsByClassName('drop')[0];
            var menuList = document.getElementById('menuList');
            ele.onclick = function () {
                appcan.window.open("zuzhi","zuzhi.html");
            };
        }

        function Mover(ele) {
            this.obj = ele;
            this.width = document.documentElement.clientWidth;
            this.height = document.documentElement.clientHeight;
            this.pageX;
            this.pageY;
            this.curLeft;
            this.curTop;
            this.activeLeft;
            this.activeTop;
            this.isDroping = false;
            var that = this;
            this.touchstart = function (e) {
                that.isDroping = true;
                that.pageX = e.touches[0].pageX;
                that.pageY = e.touches[0].pageY;
                that.curLeft = that.obj.offsetLeft;
                that.curTop = that.obj.offsetTop;
            };
            this.touchend = function () {
                that.isDroping = false;
            };
            this.touchmove = function (e) {
                if (!that.isDroping) return;
                that.activeLeft = e.touches[0].pageX - (that.pageX - that.curLeft) < 0 ? 0 : e.touches[0].pageX - (
                    that
                    .pageX - that.curLeft);
                that.activeTop = e.touches[0].pageY - (that.pageY - that.curTop) < 0 ? 0 : e.touches[0].pageY - (
                    that.pageY -
                    that.curTop);
                if (e.touches[0].pageX + that.obj.offsetWidth - (that.pageX - that.curLeft) > that.width) {
                    that.activeLeft = that.width - that.obj.offsetWidth;
                }
                if (e.touches[0].pageY + that.obj.offsetHeight - (that.pageY - that.curTop) > that.height) {
                    that.activeTop = that.height - that.obj.offsetHeight;
                }
                that.obj.style.left = that.activeLeft + 'px';
                that.obj.style.top = that.activeTop + 'px';
            }
            this.obj.ontouchstart = this.touchstart;
            this.obj.ontouchmove = this.touchmove;
            this.obj.ontouchend = this.touchend;
            document.addEventListener("touchmove", this.touchmove, true);
        }
        init();
        var mover = new Mover(document.getElementById("drop"));
    </script>

