.cpbgHead{
	width: 100%;
	height: 50px;
	font-family: MicrosoftYaHei-Bold;
	font-size: 16px;
	color: #434549;
	letter-spacing: 0;	
	text-align:center;
	line-height: 50px;
	border-bottom: 1px solid #F0F1F1;
}
.returnBtn{
    background: #FFB430;
    width: 120px;
    height: 49px;
    position: absolute;
    top: 0;
    right: 0;
    font-size: 14px;
    color: #FFFFFF;
    text-align: center;
    line-height: 50px;
    cursor: pointer;
}
.share{
	height: 50px;
	width: 100%;
}
.share img{
	margin-top: 13px;
	cursor: pointer;
}
.share img:nth-child(1){
	margin-left: 80%;
}
.share img:nth-child(2){
	margin-left: 20px;
}
.share img:nth-child(3){
	margin-left: 20px;
}
.cpbgRac{
	width: 86px;
	height: 86px;
	background: #1E70E9;
	border-radius: 50%;
	margin: 0 auto;
	text-align: center;
	line-height: 86px;
}
.cpbgRac span:nth-child(1){
	font-size: 30px;
	color: #FFFFFF;
	letter-spacing: 0;
}
.cpbgRac span:nth-child(2){
	font-size: 14px;
	color: #FFFFFF;
}
.cpbg table{
	width: 75%;
	margin: 20px auto;
}
.cpbg table tr td{
	/*width:20%;*/
	height: 42px;
	border: 1px solid #C9C9C9;
}
.cpbg table tr td:nth-child(odd){
	width: 10%;
	font-size: 14px;
	color: #434549;
	letter-spacing: 0;
	font-weight: bold;
	padding-right: 20px;
	text-align: right;
}
.cpbg table tr td:nth-child(even){
	width: 30%;
	padding-left: 20px;
}
.cpbgNav{
	width: 100%;
	height: 60px;
	border-bottom: 1px solid #d8d8d8;
}
.cpbgNav ul{
	height: 60pX;
}
.cpbgNav ul li{
	float: left;
	width: 170px;
	text-align: center;
	line-height: 60px;
	font-size: 16px;
	color: #C0C4C8;
	letter-spacing: 0;
	cursor: pointer;
}
.cpbgNav ul li.active{
	color: #1E70E9;
}
.cpbgNav ul li:hover{
	color: #1E70E9;
}
.echarts1{
	width: 100%;
	height: 370px;
}
.cpbgLine{
	position: absolute;
	width: 170px;
	height: 5px;
	background: #1E70E9;
	transition: all 1s;
}

/*四环*/
.fouthDataTitle1{
	width: 75%;
	height: 40px;
	background: #F2F2F2;
	margin: 0 auto;
	margin-bottom: 20px;
}
.fouthDataTitle1 span:nth-child(1),.fouthDataTitle1 span:nth-child(2){
	float: right;
	height: 100%;
	line-height: 40px;
	font-size: 16px;
	color: #838C95;
	margin-right: 30px;
}
.fouthDataTitle1 span i{
	font-size: 16px;
	color: #1E70E9;
	font-style: normal;
}
.fouthDataTitle1 span:nth-child(3){
	font-size: 16px;
	color: #838C95;
	height: 100%;
	line-height: 40px;
	margin-left: 30px;
	font-weight: bold;
}
.echarts2,.echarts3{
	width: 100%;
	height: 300px;
}
.echarts21,.echarts31{
	width: 56%;
	height: 100%;
	float: left;
}
.echarts22,.echarts32{
	width: 40%;
	height: 100%;
	float: left;
}
.cpbg .cpbgTab1,.cpbg .cpbgTab2{
	width: 75%;	
}
.cpbg .cpbgTab1 tr td:nth-child(1){
	width: 80%;
	text-align: left;
	padding-left: 80px;
}
.cpbg .cpbgTab1 tr td:nth-child(2),.cpbg .cpbgTab1 tr td:nth-child(3){
	width: 10%;
	font-size: 14px;
	color: #1E70E9;
	font-weight: normal;
	text-align: center;
}

.cpbg .cpbgTab2 tr td{
	color: #838C95;
}
.cpbg .cpbgTab2 tr td:nth-child(1){
	width: 15%;
	text-align: left;
	padding-left: 40px;
}
.cpbg .cpbgTab2 tr td:nth-child(2){
	width: 15%;
}
/*.cpbg .cpbgTab2 tr td:nth-child(3){
	width: 20%;
}*/
.cpbg .cpbgTab2 tr td:nth-child(4){
	width: 15%;
}
.cpbg .cpbgTab2 tr td:nth-child(5){
	width: 15%;
}
.cpbg .cpbgTab2 tr td:nth-child(6){
	width: 10%;
}
.cpbg .cpbgTab2 tr td:nth-child(7){
	width: 10%;
}
.fixBar{
	position: fixed;
	width: 120px;
	min-height: 200px;
	background: #FFFFFF;
	border: 1px solid #F0F1F1;
	box-shadow: 0 2px 4px 0 rgba(0,0,0,0.25);	
	top: 50%;
	transform: translate(0,-50%);
	right: 10px;
	padding-top: 30px;
}
.fixBar ul li{
	height: 34px;
	line-height: 34px;
	width: 100%;
	text-align: center;
	font-size: 14px;
	color: #838C95;
	cursor: pointer;
}
.fixBar ul li.active{
	background: #1E70E9;
	color: #FFF;
}
.fixBar ul li:hover{
	background: #1E70E9;
	color: #FFF;	
}

