	cursor: pointer;
}
.login .loginContent ul li.active{
	color: #FFFFFF;
}
.user,.pass{
	font-family: MicrosoftYaHei;
	font-size: 14px;
	color: #838C95;
	letter-spacing: 0;	
	margin-left: 40px;
	margin-top: 30px;
}
.userName,.password{
	width: 100%;
	height:42px;
	line-height:42px;
	outline: none;
	border: none;
	border:1px solid #d8e0e9;
	margin-top:20px;
	padding-left:50px;
	border-radius:2px;
}
.password.yzm{width:210px;}
.loginCheck{
	width: 100%;
	height: 70px;
	overflow:hidden;
}
.loginCheck2{
	font-family: MicrosoftYaHei;
	font-size: 12px;
	color: #838C95;
	letter-spacing: 0;	
	float: right;
	line-height: 70px;
	text-align: right;
	cursor: pointer;
	color:#447DFE;
}
.loginCheck2:hover{text-decoration:underline;}
.loginCheck1{
	font-family: MicrosoftYaHei;
	font-size: 14px;
	color: #1E70E9;
	letter-spacing: 0;
