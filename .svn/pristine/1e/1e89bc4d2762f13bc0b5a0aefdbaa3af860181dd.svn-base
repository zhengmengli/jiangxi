ul{
	list-style-type: none;
	margin: 0;
	padding: 0;
}

/*登录注册页*/
.login{
	position: relative;
	width: 100%;
	height: 100%;
	overflow: hidden;
	background:#edf1f5;
}
.login1{
	position: relative;
	width: 100%;
	height: 100%;
	overflow: hidden;
	background: url(../img/login/loginBg.jpg)no-repeat center;
	background-size: 100% 100%;
}
.positionC{
	position: absolute;
	top: 50%;
	transform: translate(0,-50%);
	right: 15%;
}
.loginTitle{
	font-family: MicrosoftYaHei-Bold;
	font-size: 18px;
	color: #FFFFFF;
	letter-spacing: 0;
	width: 330px;
	text-align: center;	
	margin-bottom: 20px;
	/*position: absolute;*/
	/*top: 24%;*/
	/*top:50%;
	transform: translate(0,-50%);
	right: 20%;*/
}
.loginContent{
	width: 380px;
	min-height: 300px;
	background:url('../img/login/bg3.png') no-repeat center bottom #fff;
	border-radius:5px;
	padding:30px;
	/*top: 30%;
	right: 17%;
	position: absolute;*/
}
.loginContent ul{
	width: 100%;
	height: 50px;
	background: #434549;
	box-sizing: border-box;
}
.loginContent ul li{
	width: 50%;
	height: 100%;
