.loginNone{border:1px solid red !important;}
.loginTit{overflow:hidden;line-height:40px;}
.loginTitL{float:left;font-size:24px;}
.loginTitR{float:right;}
.loginTitR a{font-weight:bold;}
.loginTitR a:hover{color:#447dfe;}
.callus{margin-top:40px;line-height:20px;height:20px;color:#447DFE;position:relative;text-align:center;}
.callus i.line1{position:absolute;top:50%;height:1px;width:120px;background:#d8e0e9;left:0;}
.callus i.line2{position:absolute;top:50%;height:1px;width:120px;background:#d8e0e9;right:0;}
.callus span{cursor:pointer;}
.loginam{animation:loginam .8s;-webkit-animation:loginam .8s;}
.loginam2{animation:loginam2 .8s;-webkit-animation:loginam2 .8s;}
.loginamLogo{animation:loginamLogo 1.5s;-webkit-animation:loginamLogo 1.5s;}
.zwh{height:10px;width:100%;}
.examineTel{background:#ddeaf9;width:320px;height:30px;line-height:30px;position:absolute;top:66px;left:30px;overflow:hidden;padding:0 10px;color:#447DFE;display:none;}
.examineTel i{float:right; cursor:pointer;}

@keyframes loginam
{
	from{transform:perspective(450px) rotateY(180deg);} 
    to{transform:perspective(450px) rotateY(0deg);} 
	/* from {
		transform: rotateY(-180deg);
		-webkit-transform: rotateY(-180deg);	Safari 和 Chrome
	}
	to {
		transform: rotateY(0);
		-webkit-transform: rotateY(0);	Safari 和 Chrome
	} */
}
@keyframes loginam2
{
	from{transform:perspective(450px) rotateY(-180deg);} 
    to{transform:perspective(450px) rotateY(0deg);} 
}
@keyframes loginamLogo
{
	from{top:100%;} 
    to{top:50%;} 
}

